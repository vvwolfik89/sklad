<div class="tab-pane fade show active" id="nav-car-info" role="tabpanel" aria-labelledby="nav-car-info-tab">


  <div class="panel">

    <!--              <p class="card-text"><%#= I18n.t("activerecord.attributes.role.role_types.#{@car_inspection.role_type}") %></p>-->
    <%= show_action_field(I18n.t("activerecord.attributes.car_inspection.inspect_lighting"), dynamic_label: { object: @car_inspection, field_name: :title }) { boolean_badge_check(@car_inspection.inspect_lighting) } %>
    <%= show_action_field(I18n.t("activerecord.attributes.car_inspection.steering_check"), dynamic_label: { object: @car_inspection, field_name: :title }) { boolean_badge_check(@car_inspection.steering_check) } %>
    <%= show_action_field(I18n.t("activerecord.attributes.car_inspection.tire_condition"), dynamic_label: { object: @car_inspection, field_name: :title }) { boolean_badge_check(@car_inspection.tire_condition) } %>
    <%= show_action_field(I18n.t("activerecord.attributes.car_inspection.body_condition"), dynamic_label: { object: @car_inspection, field_name: :title }) { boolean_badge_check(@car_inspection.body_condition) } %>
    <%= show_action_field(I18n.t("activerecord.attributes.car_inspection.checking_brake"), dynamic_label: { object: @car_inspection, field_name: :title }) { boolean_badge_check(@car_inspection.checking_brake) } %>
    <%= show_action_field(I18n.t("activerecord.attributes.car_inspection.checking_windshield"), dynamic_label: { object: @car_inspection, field_name: :title }) { boolean_badge_check(@car_inspection.checking_windshield) } %>
    <%= show_action_field(I18n.t("activerecord.attributes.car_inspection.checking_termoking"), dynamic_label: { object: @car_inspection, field_name: :title }) { boolean_badge_check(@car_inspection.checking_termoking) } %>
    <%= show_action_field(I18n.t("activerecord.attributes.car_inspection.checking_termotrans"), dynamic_label: { object: @car_inspection, field_name: :title }) { boolean_badge_check(@car_inspection.checking_termotrans) } %>
    <%= show_action_field(I18n.t("activerecord.attributes.car_inspection.checking_unitess"), dynamic_label: { object: @car_inspection, field_name: :title }) { boolean_badge_check(@car_inspection.checking_unitess) } %>
    <%= show_action_field(I18n.t("activerecord.attributes.car_inspection.notes"), dynamic_label: { object: @car_inspection, field_name: :title }) { @car_inspection.notes } %>
    <%=  @car_inspection.versions %>

  </div>

  <div class="form-group">
    <div class="col-sm-2">
      <%= link_to("Back", registers_car_inspections_path, class: "btn btn-default btn-block") %>
    </div>
    <% if can?(:edit, @car_inspection) %>
      <div class="col-sm-2">
        <%= link_to("Edit", edit_registers_car_inspection_path(@car_inspection), class: "btn btn-primary btn-block") %>
      </div>
    <% end %>
    <% if can?(:destroy, @car_inspection) %>
      <div class="col-sm-2">
        <%= link_to("Destroy", registers_car_inspections_path(@car_inspection), method: :delete, data: {confirm: "Are you sure?"}, class: "btn btn-danger btn-block") %>
      </div>
    <% end %>
  </div>
</div>
