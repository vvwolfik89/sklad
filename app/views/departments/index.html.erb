<div>
  <h1>Departments</h1>
  <%= link_to 'New dot', new_department_path %>
  <div>
    <div>
      <% @departments.each do |department| %>
        <a href="<%= department_path(department) %>">
          <h5> <%= department.title %></h5>
        </a>

          <p> <%= department.body %></p>
      <% end %>
    </div>
  </div>
</div>

